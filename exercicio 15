#include <stdio.h>

int main(){
    
   int i,j, soma, matriz[4][4];
   
   for(i=0; i<4; i++){
        for(j=0; j<4; j++){
            printf("digite");
            scanf("%d", &matriz[i][j]);
             
        soma = soma+matriz[i][j];
             

        }
   }


    printf("soma Ã© %d", soma);
       
    return 0;
}
