#include <stdio.h>

int main() {
    int a,b,c;
    
    printf("digite");
    scanf("%d %d %d", &a, &b, &c);

if( (a+b>c) && (a+c>b) && (b+c>a)){
if (a==b && b==c){
    printf("equilatero");
}    

else if(a==b||b==c||a==c){
printf("isoceles");    
}    
    
else{
printf("escaleno");    
}    
}
else{
printf("n√£o da para formar triangulo");    
}        
    

    return 0;
}
