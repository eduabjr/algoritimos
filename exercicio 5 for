#include <stdio.h>

int main() {
    int i, n, contador = 0;

    printf("Digite: ");
    scanf("%d", &n);

    for (i = 1; i < n; i++) {
        if (i%6!=0) {
            printf("%d ", i);
        } else {
            contador++;
        }
    }

    printf("\nQuantidade: %d\n", contador);

    return 0;
}
