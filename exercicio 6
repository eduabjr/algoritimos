#include <stdio.h>

int main() {
    int i, n, m;
    
    printf("Digite três números:\n");
    for (i = 0; i < 3; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &n);
        
        if (i == 0 || n > m) {
            m = n;
        }
    }        

    printf("O maior número é %d\n", m);

    return 0;
}
