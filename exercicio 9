#include <stdio.h>

int main(){
    
   int i,j, matriz[3][3];
   
   for(i=0; i<3; i++){
        for(j=0; j<3; j++){
             printf("digite");
                scanf("%d", &matriz[i][j]);


            if(i==j){
                matriz[i][j]=0;
            }
            
            else{
               matriz[i][j]; 
            }
        }
   }

   for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            printf("%d", matriz[i][j]);
        }
            printf("\n");
   }
        
       
    return 0;
}
