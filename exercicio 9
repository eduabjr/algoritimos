#include <stdio.h>

int main() {
    int i, t;

    printf("tamanho vetor:");
    scanf("%d", &t);

    int v[t];

    for (i =0; i <t ; i++) {
        printf("Digite");
        scanf("%d", &v[i]);
        if(v[i]<0){
        v[i] = 0;
        }        
    }

    for (i = 0; i < t; i++) {
        printf("%d ", v[i]);
    }

    return 0;
}
